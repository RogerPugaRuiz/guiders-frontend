{
  "name": "guiders",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --no-hmr --port=4200",
    "start:hmr": "ng serve --port=4200",
    "build": "ng build --output-path=dist/guiders",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:jest": "jest",
    "test:jest:watch": "jest --watch",
    "test:jest:coverage": "jest --coverage",
    "test:cypress": "cypress run",
    "test:cypress:headless": "cypress run --headless",
    "test:cypress:open": "cypress open",
    "serve:ssr:guiders": "node dist/guiders/server/server.mjs"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.1.0",
    "@angular/common": "^19.1.0",
    "@angular/compiler": "^19.1.0",
    "@angular/core": "^19.1.0",
    "@angular/forms": "^19.1.0",
    "@angular/platform-browser": "^19.1.0",
    "@angular/platform-browser-dynamic": "^19.1.0",
    "@angular/platform-server": "^19.1.0",
    "@angular/router": "^19.1.0",
    "@angular/ssr": "^19.1.8",
    "esbuild": "^0.25.5",
    "express": "^4.18.2",
    "lucide-angular": "^0.511.0",
    "rxjs": "~7.8.2",
    "tslib": "^2.3.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.1.8",
    "@angular/cli": "^19.1.8",
    "@angular/compiler-cli": "^19.1.0",
    "@cypress/schematic": "^3.0.0",
    "@types/express": "^4.17.17",
    "@types/jasmine": "~5.1.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^18.18.0",
    "cypress": "^14.4.0",
    "jasmine-core": "~5.5.0",
    "jest": "^29.7.0",
    "jest-preset-angular": "^14.5.5",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "ts-node": "^10.9.2",
    "typescript": "~5.7.2",
    "wait-on": "^8.0.3"
  }
}
